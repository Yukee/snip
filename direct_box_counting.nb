  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", "RenyiBc", "]"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"RenyiBc", "[", 
     RowBox[{"list_", ",", "n_", ",", "\[Delta]_", ",", "q_"}], "]"}], ":=", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"curLbl", "=", "1"}], ",", 
        RowBox[{"curBS", "=", "0"}], ",", "\[CapitalDelta]", ",", 
        RowBox[{"b", "=", 
         RowBox[{"\[Delta]", "^", 
          RowBox[{"-", "n"}]}]}], ",", 
        RowBox[{"qnorm", "=", "0"}], ",", 
        RowBox[{"ntot", "=", 
         RowBox[{"Length", "[", "list", "]"}]}], ",", 
        RowBox[{"npts", "=", "0"}], ",", 
        RowBox[{"len", "=", 
         RowBox[{"Length", "[", "list", "]"}]}], ",", "loop"}], "}"}], ",", 
