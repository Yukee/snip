Cell[CellGroupData[{

Cell["Example: R\[EAcute]nyi dimensions of the periodic chain spectrum", \
"Subsection",
 CellChangeTimes->{{3.6303218583183126`*^9, 3.630321906334482*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"compute", " ", "the", " ", "spectrum"}], " ", "*)"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"normalized", " ", "spectrum"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"spect", "[", "n_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"spec", ",", "mp", ",", "h"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"mp", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"DiscreteDelta", "[", 
           RowBox[{"j", "-", "k", "+", "1"}], "]"}], "+", 
          RowBox[{"DiscreteDelta", "[", 
           RowBox[{"j", "-", "k", "+", "n", "-", "1"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "n"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"h", "=", 
       RowBox[{
        RowBox[{"mp", "+", 
         RowBox[{"Transpose", "@", "mp"}]}], "//", "N"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"spec", "=", 
       RowBox[{"Sort", "[", 
        RowBox[{
         RowBox[{"Eigenvalues", "[", "h", "]"}], "//", "N"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"spec", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"spec", "-", 
          RowBox[{"spec", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ")"}], "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"spec", "[", 
           RowBox[{"[", 
            RowBox[{"-", "1"}], "]"}], "]"}], "-", 
          RowBox[{"spec", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ")"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "spec", "]"}], ";"}]}], "]"}]}]}]], "Input",
